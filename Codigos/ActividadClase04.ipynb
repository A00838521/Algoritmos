        "# Utiliza la técnica de programación de decrease and conquer para resolver:\n",
        "# Complejidad O(log n)\n",
        "\n",
        "def power(a, n):\n",
        "  if n == 0:\n",
        "    return 1\n",
        "\n",
        "  half = power(a, n // 2)\n",
        "  if n % 2 == 0:\n",
        "    return half * half\n",
        "  else:\n",
        "    return half * half * a\n",
        "\n",
        "print(power(2, 8))"
      ]
    }
  ]
